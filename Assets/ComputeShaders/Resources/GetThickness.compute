#pragma kernel GetThickness

Texture2D<float4> Tex;
RWStructuredBuffer<uint> ThicknessBuffer;
uint MinX;
uint MaxX;


[numthreads(32,32,1)]
void GetThickness (uint3 id : SV_DispatchThreadID)
{
    if( id.x >= MinX && id.x <= MaxX && Tex[id.xy].a > 0.0001)
    {   
        InterlockedAdd(ThicknessBuffer[id.x], 1);
    }
}